					<ccts:ComponentType>Class</ccts:ComponentType>
					<ccts:DictionaryEntryName>Concept. Details</ccts:DictionaryEntryName> <!--should we indecate the prefix? in the class Constraint it is indicated.
																								if so, the axioms should have the prefix as well.-->
					<ccts:Definition> </ccts:Definition>
					<DefinitionSource/>
					<ccts:ObjectClass>cccev: Concept</ccts:ObjectClass> <!--should we indecate the prefix? in the class Constraint it is indicated.
																								if so, the axioms should have the prefix as well.-->
				</an:Class>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="cbc:id" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<an:DataProperty>
							<ccts:ComponentType>Data property</ccts:ComponentType>
							<ccts:DictionaryEntryName>Concept. identifier</ccts:DictionaryEntryName>
							<ccts:Definition>
								<DefinitionSource/>
							</ccts:Definition>
							<ccts:Cardinality>0..*</ccts:Cardinality>
							<ccts:ObjectClass>Concept</ccts:ObjectClass>
							<ccts:PropertyTerm>Identifier</ccts:PropertyTerm>	<!--indicate prefix in the datatypes? dfference with representation term?-->
							<ccts:RepresentationTerm>Identifier</ccts:RepresentationTerm>
							<ccts:DataType>ID. Type</ccts:DataType>
						</an:DataProperty>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="cbc:name" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<an:DataProperty>
							<ccts:ComponentType>Data property</ccts:ComponentType>
							<ccts:DictionaryEntryName>Concept. name</ccts:DictionaryEntryName>
							<ccts:Definition>
								<DefinitionSource/>
							</ccts:Definition>
							<ccts:Cardinality>0..1</ccts:Cardinality>
							<ccts:ObjectClass>Concept</ccts:ObjectClass>
							<ccts:PropertyTerm>Text</ccts:PropertyTerm>
							<ccts:RepresentationTerm>Text</ccts:RepresentationTerm>
							<ccts:DataType>Name. Type</ccts:DataType>
						</an:DataProperty>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="cbc:description" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<an:DataProperty>
							<ccts:ComponentType>Data property</ccts:ComponentType>
							<ccts:DictionaryEntryName>Concept. description</ccts:DictionaryEntryName>
