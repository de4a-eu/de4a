						<an:DataProperty>
							<ccts:ComponentType>Data property</ccts:ComponentType>
							<ccts:DictionaryEntryName>Concept. type</ccts:DictionaryEntryName>
							<ccts:Definition>
								<DefinitionSource/>
							</ccts:Definition>
							<ccts:Cardinality>0..1</ccts:Cardinality>
							<ccts:ObjectClass>Concept</ccts:ObjectClass>
